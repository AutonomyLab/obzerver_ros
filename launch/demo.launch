<launch>
  <node name="obzerver_node" pkg="obzerver_ros" type="obzerver_node" clear_params="true" output="screen">
    <param name="enable_debug_image" value="true" />
    <param name="enable_diff_image" value="true" />
    <param name="enable_simmat_image" value="true" />
    <param name="enable_stablized_image" value="true" />

    <param name="obz_logfile" value="/tmp/mani" />
    <param name="downsample_factor" value="0.5" />
    <param name="queue_size" value="1" />

    <remap from="obzerver/image" to="ardrone/image_raw" />
  </node>
</launch>
